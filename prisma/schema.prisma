datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          Int     @default(autoincrement()) @id
  asin        String  @unique
  price       String  @default("0.00")
  category    String
  type        String
  name        String 
  slug        String  @unique 
  imageUrl    String 
  url        String
  description String? 
  features    String
}
