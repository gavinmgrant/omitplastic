datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          Int     @default(autoincrement()) @id
  asin        String  @unique
  price       String  @default("0.00")
  category    String
  type        String
  name        String 
  slug        String  @unique 
  imageUrl    String 
  urls        Json[]
  description String? 
  features    String[]
}
